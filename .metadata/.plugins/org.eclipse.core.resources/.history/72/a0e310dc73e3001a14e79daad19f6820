package helloword;

public class helloword {
	public static void main (String[] args) {
			String name = "linjing";
			String name2 = name.substring(0,name.length());
			System.out.println(name2);
			if(name.equals(name2)) {
				System.out.println("---------------------");
			}
			if(name==name2)) {
				System.out.println("---------------------");
			}
	}
}
