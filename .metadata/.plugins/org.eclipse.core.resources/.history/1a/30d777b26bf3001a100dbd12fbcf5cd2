package helloword2;

class Mother{
	public void eat() {
		System.out.println("eat");
	}
}
class son extends Mother{
	
	public void eat() {
		System.out.println("eat2 ...");
	}
	public void eat(String str) {
		System.out.println(str);
	}
}
public class overwrite {

		public static void main(String[] args) {
		
			son s = new son();
			s.eat();
			s.eat("sssss");
		}
}
