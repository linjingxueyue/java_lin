package helloword2;
/**
 *  main() 方法的使用说明：
 *  	1： 作为程序的入口
 *  	2： 也是一个普通的静态方法。
 *  	
 * @author PC
 *
 */
public class main_xuexi {
	public static void main(String[] args) {	// 入口
		Main.main(new int[100]);
	}
}
class Main{
	public static void main(String[] args) {
		for(int i=0;i<args.length;i++) {
			args[i]="args_"+i;
			System.out.println(args[i]);
		}
	}	
}