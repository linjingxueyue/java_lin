class son2{
	protected  String name = "sad";
}
public class Son1 extends son4_object_01 {
	public static void main(String[] args) {
		son2 s2 = new son2();
		System.out.println(s2.name);
		son4_object_01 s1 = new son4_object_01();
		System.out.println(s1.name4);
	}
}